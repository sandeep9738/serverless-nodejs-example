{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "SignIn Schema",
  "required": ["fullname", "email","password"],
  "properties": {
    "fullname": {
      "type": "string",
      "title": "Fullname of the User"
    },
    "phone": {
      "type": "string",
      "title": "Phone number",
      "minLength" : 10
    },
    "email": {
      "type": "string",
      "title": "Email address of the User",
      "format": "email"
    },
    "password": {
      "type": "string",
      "title": "Password of user"
    }
  }
}
